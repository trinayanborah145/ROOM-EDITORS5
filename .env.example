# API Configuration
REACT_APP_API_URL=http://localhost:5000/api/v1

# AWS Configuration (for direct S3 uploads)
REACT_APP_AWS_REGION=your-aws-region
REACT_APP_AWS_BUCKET_NAME=your-s3-bucket-name
